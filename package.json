{
  "name": "isomorphic-react",
  "version": "1.0.0",
  "description": "A isomorphic react app with typescript",
  "main": "server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run server:dev\" \"npm run webpack:dev\"",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "webpack:dev": "webpack serve --config ./config/webpack.dev.client.js",
    "server:dev": "webpack --config ./config/webpack.dev.server.js --watch",
    "webpack": "webpack"
  },
  "author": "Jordan Kelly",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^6.0.0",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "assets-webpack-plugin": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-node": "0.0.1-security",
    "clean-webpack-plugin": "^3.0.0",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mini-css-extract-plugin": "^1.3.9",
    "nodemon": "^2.0.7",
    "nodemon-webpack-plugin": "^4.4.4",
    "sass-loader": "^11.0.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2"
  }
}
